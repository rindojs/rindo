{
  "name": "@rindo/core",
  "version": "4.0.4",
  "license": "MIT",
  "main": "./internal/rindo-core/index.cjs",
  "module": "./internal/rindo-core/index.js",
  "types": "./internal/rindo-core/index.d.ts",
  "bin": {
    "rindo": "bin/rindo"
  },
  "files": [
    "!**/*.map",
    "!**/*.stub.ts",
    "!**/*.stub.tsx",
    "bin/",
    "cli/",
    "compiler/",
    "dev-server/",
    "internal/",
    "mock-doc/",
    "screenshot/",
    "sys/",
    "testing/"
  ],
  "scripts": {
    "build": "npm run tsc.scripts && npm run tsc.prod && npm run rollup.prod.ci",
    "// changelog": "conventional-changelog -p angular -o -i CHANGELOG.md -s",
    "clean": "rm -rf build/ cli/ compiler/ dev-server/ internal/ mock-doc/ sys/ testing/ && npm run clean-scripts",
    "clean-scripts": "rm -rf scripts/build",
    "license": "node scripts/build --license",
    "lint": "eslint \"bin/*\" \"scripts/*.ts\" \"scripts/**/*.ts\" \"src/*.ts\" \"src/**/*.ts\"",
    "prettier": "npm run prettier.base -- --write",
    "prettier.base": "prettier --cache \"./({bin,scripts,src}/**/*.{ts,tsx,js,jsx})|bin/rindo|.github/(**/)?*.(yml|yaml)|*.js\"",
    "prettier.dry-run": "npm run prettier.base -- --list-different",
    "release": "npm run tsc.scripts && node scripts/build --release --publish",
    "release.ci.prepare": "npm run tsc.scripts && NODE_OPTIONS=--max-old-space-size=4096 node scripts/build --release --ci-prepare",
    "release.ci": "npm run tsc.scripts && node scripts/build --release --ci-publish",
    "release.prepare": "npm run tsc.scripts && node scripts/build --release --prepare",
    "rollup": "rollup --config",
    "rollup.prod": "rollup --config --config-prod",
    "rollup.prod.ci": "rollup --config --config-prod --config-ci",
    "rollup.watch": "rollup --watch --config",
    "start": "npm run watch",
    "tsc": "tsc --incremental",
    "tsc.prod": "tsc",
    "tsc.scripts": "tsc --build --force scripts/tsconfig.json",
    "tsc.watch": "tsc --incremental --watch"
  },
  "devDependencies": {
    "@familyjs/prettier-config": "^4.0.0",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.4",
    "@rollup/pluginutils": "5.0.2",
    "@types/eslint": "^8.4.6",
    "@types/exit": "^0.1.31",
    "@types/fs-extra": "^11.0.0",
    "@types/glob": "^8.0.0",
    "@types/graceful-fs": "^4.1.5",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^27.0.3",
    "@types/listr": "^0.14.4",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^20.1.1",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/prompts": "^2.0.9",
    "@types/semver": "^7.3.12",
    "@types/sizzle": "^2.3.2",
    "@types/ws": "^8.5.4",
    "@types/yarnpkg__lockfile": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@yarnpkg/lockfile": "^1.1.0",
    "ansi-colors": "4.1.3",
    "autoprefixer": "10.4.14",
    "conventional-changelog-cli": "^3.0.0",
    "dts-bundle-generator": "~8.0.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-jsdoc": "^46.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "execa": "7.1.1",
    "exit": "^0.1.2",
    "fs-extra": "^11.0.0",
    "glob": "8.1.0",
    "graceful-fs": "~4.2.6",
    "inquirer": "^7.3.3",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "jest-environment-node": "^27.4.4",
    "listr": "^0.14.3",
    "magic-string": "^0.30.0",
    "merge-source-map": "^1.1.0",
    "mime-db": "^1.46.0",
    "minimatch": "5.1.6",
    "mock-fs": "^5.2.0",
    "node-fetch": "3.3.2",
    "open": "^9.0.0",
    "open-in-editor": "2.2.0",
    "parse5": "7.1.2",
    "pixelmatch": "5.3.0",
    "postcss": "^8.2.8",
    "prettier": "3.0.1",
    "prompts": "2.4.2",
    "puppeteer": "^20.0.0",
    "rollup": "2.42.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "semver": "^7.3.7",
    "sizzle": "^2.3.6",
    "terser": "5.19.2",
    "typescript": "~5.0.4",
    "webpack": "^5.75.0",
    "ws": "8.13.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/familyjs/rindo.git"
  },
  "author": "Family Team",
  "homepage": "https://rindojs.web.app/",
  "description": "A Compiler for Web Components and Progressive Web Apps",
  "keywords": [
    "web components",
    "components",
    "rindo",
    "familyjs",
    "webapp",
    "custom elements",
    "pwa",
    "progressive web app"
  ],
  "prettier": "@familyjs/prettier-config",
  "volta": {
    "node": "20.4.0",
    "npm": "9.8.1"
  }
}
